  <property name="styleSheet">
   <string notr="true">* {
	border: none;
	background-color: transparent;
	padding: 0;
	margin: 0;
	color: #fff;
}

#centralwidget, #instruction_btn, #home_btn, #algorithm_btn, #main_content, QLineEdit {
	background-color: #1b1b27;
}

#header, #main_body {
	background-color: #27263c;
}

#footer {
	background-color: #000;
}

#add_user_btn {
	background-color: #1b1b27;
	border-radius: 10px;
}

#pushButton {
	border: 1px solid #cc5bce;
	border-radius: 16px;
	text-align: center;
}

QPushButton {
	text-align: left;
	padding: 3px 10px;
	border-top-left-radius: 10px;
	border-bottom-left-radius: 10px;
	border-top-right-radius: 10px;
	border-bottom-right-radius: 10px;
}

#home_btn, #reports_btn, #algorithm_btn, #instruction_btn, #account_btn  {
	border-left: 3px solid #cc5bce;
	font-weight: bold;
}

QLineEdit {
	padding: 5px 10px;
	border-radius: 5px;
}

QComboBox {
    background-color: white; /* Белый фон */
    color: black; /* Цвет текста */
}

QComboBox QAbstractItemView {
    background-color: white; /* Белый фон выпадающего списка */
    color: black; /* Цвет текста в выпадающем списке */
}

/*----------------------------------*/

#main_content QPushButton {
    background-color: #3a3a5c;
    color: #ffffff;
    border: 1px solid #3a3a5c;
    padding: 5px;
	
	text-align: center;
}

#main_content QPushButton:hover {
    background-color: #4a4a6c;
}

#main_content QPushButton:pressed {
    background-color: #2a2a4c;
}

#main_content QLabel {
    color: #ffffff;
}

#main_content QComboBox {
    background-color: #2a2a4c;
    color: #ffffff;
    border: 1px solid #3a3a5c;
    padding: 5px;
}

#main_content QLineEdit {
    background-color: #2a2a4c;
    color: #ffffff;
    border: 1px solid #3a3a5c;
    padding: 5px;
}

#main_content QLineEdit:focus {
    border: 1px solid #4a4a6c;
}

#main_content QProgressBar {
    background-color: #1b1b27;
    color: #ffffff;
    border: 1px solid #3a3a5c;
}

#main_content QProgressBar::chunk {
    background-color: #3a3a5c;
}

#main_content QScrollBar:vertical {
    background-color: #1b1b27;
}

#main_content QScrollBar::handle:vertical {
    background-color: #3a3a5c;
}
#main_content QLabel {
    color: #ffffff; /* Белый цвет текста */
    padding: 5px; /* Отступы внутри метки */
}

#main_content QLabel:hover {
    color: #a0a0a0; /* Изменение цвета текста при наведении */
}

/* Если нужно стилизовать конкретные метки (например, с определенным objectName) */
#main_content QLabel#specialLabel {
    color: #ffcc00; /* Желтый цвет для особой метки */
    font-weight: bold; /* Жирный шрифт */
}

#footer {
    background: qradialgradient(
        cx: 0.5, cy: 0.5, radius: 1, /* Центр и радиус градиента */
        fx: 0.5, fy: 0.5,            /* Фокус градиента */
        stop: 0 #1b1b27,
        stop: 1 #27263c
    );
}

/* CSS FOR QTABLEWIDGET*/
/* Основная таблица */
QTableWidget {
    background-color: #1b1b27;
    border: 1px solid #3a3a5c;
    border-radius: 4px;
    gridline-color: #3a3a5c;
    font-size: 14px;
}

/* Заголовки столбцов */
QHeaderView::section {
    background-color: #27263c;
    color: #ffffff;
    padding: 8px;
    border: none;
    border-bottom: 2px solid #3a3a5c;
    font-weight: bold;
}

/* Строки */
QTableWidget::item {
    color: #ffffff;
    background-color: #1b1b27;
    padding: 6px;
}

/* Альтернативные строки */
QTableWidget::item:alternate {
    background-color: #252536;
}

/* Выделенная строка/ячейка */
QTableWidget::item:selected {
    background-color: #3a3a5c;
    color: #ffffff;
}

/* Полосы прокрутки */
QScrollBar:vertical {
    background: #1b1b27;
    width: 12px;
}

QScrollBar::handle:vertical {
    background: #3a3a5c;
    min-height: 20px;
    border-radius: 6px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background: none;
}

/* Угол таблицы (между заголовками) */
QTableCornerButton::section {
    background-color: #27263c;
    border: none;
    border-bottom: 2px solid #3a3a5c;
}
QTableWidget::item:hover {
    background-color: #32324c;
}
/* END CSS FOR QTABLEWIDGET*/</string>
